@font-face
  font-family 'Avenir-Medium'
  src url('/assets/fonts/Avenir-Medium.eot')
  src url('/assets/fonts/Avenir-Medium.eot?#iefix') format('embedded-opentype'),
      url('/assets/fonts/Avenir-Medium.woff') format('woff'),
      url('/assets/fonts/Avenir-Medium.ttf') format('truetype')
  font-weight 500
  font-style normal

@font-face
  font-family 'Avenir-Black'
  src url('/assets/fonts/AvenirLT-Black.eot')
  src url('/assets/fonts/AvenirLT-Black.eot?#iefix') format('embedded-opentype'),
      url('/assets/fonts/AvenirLT-Black.woff') format('woff'),
      url('/assets/fonts/AvenirLT-Black.ttf') format('truetype')
  font-weight bold
  font-style normal